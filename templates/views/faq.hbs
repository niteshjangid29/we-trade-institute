    <style>
        /* -----------------FAQ------------------ */
        .faq {
            max-width: 1000px;
            margin: 50px auto;
            text-align: center;
            padding-top: 50px;
        }

        .faq .container .row h1 {
            margin-bottom: 40px;
            color: #000;
            font-weight: 700;
        }
        .faq .container .row h1 .fa{
            color: #ff2d3b;
        }

        .faq .container .row h1 span {
            color: #ff2d3b;
        }

        .faq .accordion {
            text-align: left;
        }

        .faq .insta .fa {
            color: #000;
            margin-top: 5px;
            margin-left: 10px;
            padding: 2.5px 3px;
            font-size: 20px;
            font-weight: 500;
            transition: 0.5s ease-out;
        }

        .faq .insta .fa:hover {
            color: #fff;
            background: #ff2d3b;
            font-weight: 500;
            border-radius: 5px;
        }

        .title {
            position: relative;
            margin: 10px 0 20px 0;
            line-height: 60px;
            font-weight: 600;
            color: #ff2d3b;
        }

        .title::after {
            content: '';
            background: #ff2d3b;
            width: 56%;
            height: 3.5px;
            border-radius: 4px;
            position: absolute;
            bottom: -3px;
            left: 50%;
            transform: translateX(-50%);
        }

        .accordion-item {
            margin: 10px;
            border: 2px solid #c5c5c5 !important;
        }

        .accordion-body {
            color: #606060;
        }
    </style>
